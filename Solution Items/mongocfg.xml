<?xml version="1.0" encoding="utf-8" ?>
<MongoStorageConfigs xmlns="https://github.com/bigsupersniper" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 
  <!--
    Name：配置名称
    MaxConnectionPoolSize ： 连接池大小（默认：100）
    ReplicaSetName ： 副本集名称
  -->
  <MongoStorageConfig Name="" MaxConnectionPoolSize="100" ReplicaSetName="">
    <!--连接地址（可多个）-->
    <ServerAddress></ServerAddress>
    <!--
        Name：数据库配置别名（存储类中用）
        DatabaseName：实际数据库名称
        UserName ：数据库用户名（可空）
        Password ：数据库密码（可空）
    -->
    <DatabaseConfig Name="" DatabaseName="" UserName="" Password="" />        
  </MongoStorageConfig>
  
</MongoStorageConfigs>